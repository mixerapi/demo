{
    "name": "mixerapi/demo",
    "description": "MixerAPI Demo",
    "homepage": "https://mixerapi.com",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "cakephp/authentication": "^3.0",
        "cakephp/cakephp": "^5.0",
        "cakephp/migrations": "^4.0",
        "cakephp/plugin-installer": "^2.0",
        "cnizzardini/cakephp-preloader": "^1.0.",
        "cnizzardini/cakephp-swagger-bake": "^3.0",
        "dereuromark/cakephp-setup": "^3.0",
        "firebase/php-jwt": "^6.2",
        "friendsofcake/search": "^7.0",
        "mixerapi/crud": "2.x-dev",
        "mixerapi/jwt-auth": "2.x-dev",
        "mixerapi/mixerapi": "2.x-dev",
        "mobiledetect/mobiledetectlib": "^2.8"
    },
    "require-dev": {
        "cakephp/bake": "^3.0",
        "cakephp/debug_kit": "^5.0",
        "dereuromark/cakephp-ide-helper": "^2.0",
        "josegonzalez/dotenv": "^3.2",
        "phpunit/phpunit": "^10.1",
        "psy/psysh": "@stable",
        "vierge-noire/cakephp-fixture-factories": "^3.0"
    },
    "suggest": {
        "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
        "dereuromark/cakephp-ide-helper": "After baking your code, this keeps your annotations in sync with the code evolving from there on for maximum IDE and PHPStan/Psalm compatibility.",
        "phpstan/phpstan": "PHPStan focuses on finding errors in your code without actually running it. It catches whole classes of bugs even before you write tests for the code."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "AdminApi\\": "plugins/AdminApi/src/",
            "AuthenticationApi\\": "plugins/AuthenticationApi/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
            "AdminApi\\Test\\": "plugins/AdminApi/tests/",
            "AuthenticationApi\\Test\\": "plugins/AuthenticationApi/tests/"
        }
    },
    "scripts": {
        "check": [
            "@test",
            "@cs-check"
        ],
        "cs-check": "phpcs --colors -p  src/ tests/",
        "cs-fix": "phpcbf --colors -p src/ tests/",
        "stan": "phpstan analyse",
        "test": "phpunit --colors=always"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cakephp/plugin-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../dev/plugins/bake",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/collection-view",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/crud",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/exception-render",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/hal-view",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/jwt-auth",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/json-ld-view",
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../dev/plugins/mixerapi",
            "options": {
                "symlink": false
            }
        }
    ]
}
